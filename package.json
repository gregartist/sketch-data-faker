{
  "name": "sketch-data-faker",
  "version": "0.4.0",
  "description": "A Sketch plugin providing 100+ types of realistic (fake) data for your mockups from Faker.js and other sources.",
  "engines": {
    "sketch": ">=3.0"
  },
  "author": "Paul Wright",
  "homepage": "https://sketchfaker.com",
  "identifier": "com.paintedbicycle.sketch.sketchfaker",
  "appcast": ".appcast.xml",
  "icon": "icon.png",
  "skpm": {
    "name": "Sketch Data Faker",
    "manifest": "src/manifest.json",
    "main": "sketch-data-faker.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paintedbicycle/sketch-data-faker.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.15"
  },
  "dependencies": {
    "faker": "^4.1.0"
  }
}
